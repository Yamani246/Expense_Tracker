{
    "version": 2,
    "builds": [
      {
        "src": "expense_tracker/wsgi.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.9" }
      },
      {
        "src": "FrontEnd/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/expense_tracker/wsgi.py" },
      { "src": "/(.*)", "dest": "/FrontEnd/$1" }
    ]
  }
  